<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Files">
    <insert id="upload" useGeneratedKeys="true" keyProperty="id">
        insert into files values(#{id}, #{oriName}, #{sysName}, #{parentId})
    </insert>
    <select id="selectByBoardId" resultType="kh.study.khspring.entity.Files">
        select * from files where parent_id = #{boardId}
    </select>
</mapper>