package students;

import java.util.stream.Collectors;
import java.util.stream.Stream;

public class Test1 {

	public static void main(String[] args) {
		String inputTime = "23-03-03 16/05";
		
		
		
		System.out.println(
			inputTime.codePoints()
				.filter(ch -> !(ch == '-' || ch == '/' || ch == ' '))
				.toArray()
				.toString()
		);
